    "\n",
    "if nombre:  # Verifica que la entrada no esté vacía\n",
    "    repeticiones = int(input(\"¿Cuántas veces desea repetir el nombre? \"))\n",
    "    for _ in range(repeticiones):\n",
    "        print(nombre)\n",
    "else:\n",
    "    print(\"No se introdujo un nombre.\")\n"
